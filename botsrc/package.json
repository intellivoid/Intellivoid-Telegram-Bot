{
    "package": {
        "package_name": "net.intellivoid.intellivoid_bot",
        "name": "Intellivoid Bot",
        "version": "1.0.0.3",
        "author": "Zi Xing Narrakas",
        "organization": "Intellivoid",
        "description": "The main IntellivoidBot for Intellivoid Accounts and Intellivoid related services.",
        "url": "https://github.com/intellivoid/Intellivoid-Telegram-Bot",
        "dependencies": [
            {
                "package": "net.intellivoid.acm",
                "version": "latest",
                "source": "default@github/intellivoid/acm",
                "required": true
            },
            {
                "package": "net.intellivoid.accounts",
                "version": "latest",
                "source": "default@github/intellivoid/intellivoid-accounts",
                "required": true
            },
            {
                "package": "net.intellivoid.background_worker",
                "version": "latest",
                "source": "default@github/intellivoid/BackgroundWorker",
                "required": true
            },
            {
                "package": "net.intellivoid.deepanalytics",
                "version": "latest",
                "source": "default@github/intellivoid/DeepAnalytics",
                "required": true
            },
            {
                "package": "net.intellivoid.telegram_client_manager",
                "version": "latest",
                "source": "default@github/intellivoid/IVA-Telegram",
                "required": true
            },
            {
                "package": "net.intellivoid.verbose_adventure",
                "version": "latest",
                "source": "default@github/intellivoid/VerboseAdventure",
                "required": true
            },
            {
                "package": "net.intellivoid.tdlib",
                "version": "latest",
                "source": "default@github/intellivoid/tdlib",
                "required": true
            }
        ],
        "configuration": {
            "autoload_method": "generated_spl",
            "main": {
                "execution_point": "main",
                "create_symlink": false,
                "name": null
            },
            "post_installation": [],
            "pre_installation": []
        }
    },
    "components": [
        {
            "required": true,
            "file": "IntellivoidBot.php"
        },
        {
            "required": true,
            "file": "commands/StartCommand.php"
        },
        {
            "required": true,
            "file": "commands/UnlinkCommand.php"
        },
        {
            "required": true,
            "file": "commands/WhoisCommand.php"
        },
        {
            "required": true,
            "file": "commands/CallbackqueryCommand.php"
        }
    ],
    "files": [
        "package.json",
        "worker",
        ".htaccess",
        "main"
    ]
}